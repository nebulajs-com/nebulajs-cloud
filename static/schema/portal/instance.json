                    "id": "u:667dac86f05e",
                    "value": 1,
                    "placeholder": "-",
                    "align": "center",
                    "map": {
                        "0": "nb-icon nb-icon-stop-fill text-danger",
                        "2": "nb-icon nb-icon-success-fill text-success"
                    },
                    "labelMap": {
                        "0": "已停止",
                        "2": "运行中"
                    }
                },
                {
                    "type": "operation",
                    "label": "操作",
                    "width": 150,
                    "align": "center",
                    "id": "u:5d74ca4ac5ba",
                    "buttons": [
                        {
                            "type": "button",
                            "label": "启动",
                            "actionType": "ajax",
                            "confirmText": "确定要启动实例吗？",
                            "api": {
                                "method": "get",
                                "url": "/portal/cl-instance/start/${id}",
                                "messages": {
                                    "success": "启动成功"
                                }
                            },
                            "reload": "u:c43a48194aab",
                            "id": "u:affdd1aeafb3",
                            "hiddenOn": "status=='2'",
                            "loading": true,
                            "level": "link",
                            "size": "sm"
                        },
                        {
                            "type": "button",
                            "label": "停止",
                            "actionType": "ajax",
                            "confirmText": "确定要停止实例吗？",
                            "api": {
                                "method": "get",
                                "url": "/portal/cl-instance/stop/${id}",
                                "messages": {
                                    "success": "停止成功"
                                }
                            },
                            "reload": "u:c43a48194aab",
                            "id": "u:1fbfbaa82f38",
                            "level": "link",
                            "hiddenOn": "status=='0'",
                            "size": "sm"
                        },
                        {
                            "type": "button",
                            "label": "详情",
                            "actionType": "link",
                            "link": "/portal/instance-info?id=${id}",
                            "level": "link",
                            "id": "u:8bbf6e133dcf",
                            "className": "text-gray-400"
                        },
                        {
                            "label": "删除",
                            "type": "button",
                            "actionType": "ajax",
                            "level": "link",
                            "id": "u:d4835693d45b",
                            "className": "text-danger",
                            "confirmText": "确定要删除？",
                            "api": {
                                "method": "delete",
                                "url": "/portal/cl-instance/${id}",
                                "messages": {
                                    "success": "删除成功"
                                }
                            }
                        }
                    ]
                }
