version: '3.7'
services:
    <%=name%>:
        # container_name: redis
        image: redis:6.2.1
        restart: unless-stopped
        command:
            - /bin/bash
            - -c
            - redis-server --requirepass "<%=extra.middleware.password%>"
        ports:
            - <%=extra.middleware.port%>:6379
